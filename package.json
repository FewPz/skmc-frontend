{
  "name": "skmc-monorepo",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "dotenv -e ./.env -- turbo run build --filter=frontend --filter=backend",
    "dev": "dotenv -e ./.env -- turbo run dev --filter=frontend --filter=backend",
    "lint": "turbo run lint",
    "install": "pnpm install --recursive",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "prettier": "^3.3.3",
    "turbo": "^2.1.3"
  },
  "packageManager": "pnpm@9.6.0",
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.1"
  }
}
